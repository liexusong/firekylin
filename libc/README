
This is a simple implement of libc for Firekyln kernel.

assert:	assert()	__bad_assert()

ctype:	isupper()
	islower()
	isdigit()
	iscntrl()
	isspace()
	ispunct()
	isxdigit()
	isalpha()
	isgraph()
	isprint()
	isascii()
	tolower()
	toupper()
	toascii()

string:	memchr()
	memcmp()
	memset()
	memcpy()
	memmove()
	strlen()
	strcat()
	strcpy()
	strcmp()
	stricmp()
	strnlen()
	strncat()
	strncpy()
	strncmp()
	strnicmp()
	strchr()
	strrchr()
	strstr()
	strerror()

math:	sin()
	cos()
	tan()
	asin()
	acos()
	atan()
	atan2()
	exp()
	pow()
	ln()
	lg()
	log()
	sinh()
	cosh()
	tanh()
	asinh()
	acosh()
	atanh()
	sqrt()
	hypot()
	fabs()
	floor()
	ceil()
	fmod()
	ldexp()
	
dirent:	opendir()
	readdir()
	rewinddir()
	closedir()
	
stdlib:	abs()
	labs()
	malloc()
	free()
	getenv()
	
	